{
  "name": "angular2-webpack-seed",
  "version": "1.0.0",
  "description": "A slightly more advanced Angular 2 starter seed using Webpack",
  "keywords": [
    "angular2",
    "webpack",
    "rxjs",
    "typescript",
    "seed",
    "boilerplate"
  ],
  "scripts": {
    "build": "rimraf dist && webpack --progress --profile --bail",
    "server": "webpack-dev-server --inline --progress --hot",
    "start": "npm run server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "author": "Maarten Tibau <github@webtrix.be>",
  "homepage": "https://github.com/webtrix/angular2-webpack-seed",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/router": "3.0.2",

    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "angular2-router-loader": "0.2.2",
    "angular2-template-loader": "0.5.0",
    "autoprefixer": "6.4.1",
    "codelyzer": "1.0.0-beta.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.25.0",
    "es6-promise": "3.3.1",
    "es6-shim": "0.35.1",
    "es7-reflect-metadata": "1.6.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "node-sass": "3.10.0",
    "null-loader": "0.1.1",
    "postcss-loader": "0.13.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.1",
